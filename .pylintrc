[MASTER]

[MESSAGES CONTROL]
disable = 
    missing-docstring,
    invalid-name,
    unused-import,
    line-too-long,
    too-many-arguments,
    too-many-instance-attributes,
    too-many-locals,
    too-many-branches,
    too-many-statements,
    import-outside-toplevel,
    import-error,
    too-few-public-methods,
    cyclic-import

[TYPECHECK]

[VARIABLES]

[SIMILARITIES]

[FORMAT]

[DESIGN]

[IMPORTS]
ignored-modules = flask_*

